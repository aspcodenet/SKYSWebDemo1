@page
@model SKYSWebDemo1.Pages.PlayersModel
@{
}

<h1>Alla spelare</h1>

<table class="table">
    <thead>
    <tr>
        <th scope="col">Namn</th>
        <th scope="col">Jersey</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var player in Model.Players)
    {
        <tr>
            <td>@player.Namn</td>  
            <td>@player.Jersey</td>  
        </tr>
    }

    </tbody>
</table>



